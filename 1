package main

import "golang.org/x/tour/pic"

func Pic(dx, dy int) [][]uint8 {
    var result = [][]uint8{}
    x := uint8(10)
    y := uint8(10)
    for i := 0 ; i <  dy; i++ {
	result = append(result, []uint8{})
	for j:=0;  j < dx; j++ {
		result[i] = append(result[i] , x * y)
	}
    }
    return result 
}

func main() {
	pic.Show(Pic)
}

